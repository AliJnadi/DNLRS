clc

disp('Singularity check')
disp('-----------------')

disp('Shoulder Singularity check')
disp('Jn(3.1416, -1.2269 ,1.5136 ,3.1416, 1.8575, 0)')
J = Jn(3.1416, -1.2269 ,1.5136 ,3.1416, 1.8575, 0);
fprintf('Jacobian rank %d.\n', rank(J));
fprintf('Jacobian det %d.\n', det(J));
[~,s,~] = svd(J);
fprintf('SVD rank %d.\n', rank(s));
disp('-----------------')

disp('Elbow Singularity check')
disp('Jn(0, pi/2, -pi/2, 0, 0.1, 0)')
J = Jn(0, pi/2, -pi/2, 0, 0.1, 0);
fprintf('Jacobian rank %d.\n', rank(J));
fprintf('Jacobian det %d.\n', det(J));
[~,s,~] = svd(J);
fprintf('SVD rank %d.\n', rank(s));
disp('-----------------')


disp('Wrist Singularity check')
disp('Jn(0, 0, 0, 0, 0, 0)')
J = Jn(0, 0, 0, 0, 0, 0);
fprintf('Jacobian rank %d.\n', rank(J));
fprintf('Jacobian det %d.\n', det(J));
[~,s,~] = svd(J);
fprintf('SVD rank %d.\n', rank(s));
disp('-----------------')